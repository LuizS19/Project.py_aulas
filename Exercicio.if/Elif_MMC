height = 1.85
weight = 85

dec_bmi = weight / height ** 2
bmi = round(dec_bmi)

if dec_bmi < 18.5:
    print("underweight") #(abaixo do peso)
elif 18.5 <= dec_bmi < 25:
    print("normal weight") #(peso normal)
elif 25 <= dec_bmi < 30:
    print("slightly overweight") #(levemente acima do peso)
elif 30 <= dec_bmi < 35:
    print("obese") #(obeso)
else:
    print("clinically obese") #(clinicamente obeso)
